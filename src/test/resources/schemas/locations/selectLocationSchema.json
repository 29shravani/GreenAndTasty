{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "data": {
      "type": "object",
      "properties": {
        "locationOptions": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string"
              },
              "address": {
                "type": "string"
              }
            },
            "required": ["id", "address"]
          }
        }
      },
      "required": ["locationOptions"]
    }
  },
  "required": ["data"]
}
